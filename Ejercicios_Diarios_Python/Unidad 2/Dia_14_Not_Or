n1 = float(input("Dime un numero\n"))
n2 = float(input("Dime otro\n"))
n3 = float(input("Dime otro\n"))
print(f"Hay por lo menos dos numeros iguales?\n {n1 == n2 or n1 == n3 or n2 == n3}")
print (f"Hay algun numero negativo?\n {n1<0 or n2<0 or n3<0}")
print (f"Hay alguno que sea 0?\n {not n1 != 0 or not n2 != 0 or not n3 != 0} ")
